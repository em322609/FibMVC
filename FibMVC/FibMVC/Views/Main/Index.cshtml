@model FibMVC.Biz.Fib

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Welcome to the web page to print a fibonacci sequence.</h1>
        <ul>
           @for (int i = 0; i < 15; i++)
           {
               List<int> list = (List<int>)ViewData["fifteenFibs"];
               <li>@list[i]</li>
           }
        </ul>
    </div>
</body>
</html>
